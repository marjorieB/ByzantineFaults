#!/bin/bash

cd ../../bin


#TEST FOR ARANTES FIRST_FIT NOT_RANDOM_TARGET_LOC

nb_workers=1500

# test for the first_fit symmetrical

for nb_clients in 1000 2000 4000 6000 8000 10000
do
	./my_boinc ../database_file/boinc_file $nb_workers $nb_clients 13 ../platforms/deployement_centralized_$nb_workers\workers_$nb_clients\clients.xml ../platforms/platform_centralized_$nb_workers\workers_$nb_clients\clients.xml 21600000000000 1000000000 CENTRALIZED ARANTES SYMMETRICAL 0.10 FIRST_FIT 3 13 NOT_RANDOM 0.05
done



# test for the first_fit asymmetrical

for nb_clients in 1000 2000 4000 6000 8000 10000
do
	./my_boinc ../database_file/boinc_file $nb_workers $nb_clients 13 ../platforms/deployement_centralized_$nb_workers\workers_$nb_clients\clients.xml ../platforms/platform_centralized_$nb_workers\workers_$nb_clients\clients.xml 21600000000000 1000000000 CENTRALIZED ARANTES ASYMMETRICAL 0.10 0.20 FIRST_FIT 3 13 NOT_RANDOM 0.05
done



# test for the first_fit boinc
for nb_clients in 1000 2000 4000 6000 8000 10000
do
	./my_boinc ../database_file/boinc_file $nb_workers $nb_clients 13 ../platforms/deployement_centralized_$nb_workers\workers_$nb_clients\clients.xml ../platforms/platform_centralized_$nb_workers\workers_$nb_clients\clients.xml 21600000000000 1000000000 CENTRALIZED ARANTES BOINC FIRST_FIT 3 13 NOT_RANDOM 0.05
done



# test for the first_fit sonnek_reputation

for nb_clients in 1000 2000 4000 6000 8000 10000
do
	./my_boinc ../database_file/boinc_file $nb_workers $nb_clients 13 ../platforms/deployement_centralized_$nb_workers\workers_$nb_clients\clients.xml ../platforms/platform_centralized_$nb_workers\workers_$nb_clients\clients.xml 21600000000000 1000000000 CENTRALIZED ARANTES SONNEK_REPUTATION FIRST_FIT 3 13 NOT_RANDOM 0.05
done



#TEST FOR ARANTES RANDOM_TARGET_LOC

# test for the first_fit symmetrical

for nb_clients in 1000 2000 4000 6000 8000 10000	
do
	./my_boinc ../database_file/boinc_file $nb_workers $nb_clients 13 ../platforms/deployement_centralized_$nb_workers\workers_$nb_clients\clients.xml ../platforms/platform_centralized_$nb_workers\workers_$nb_clients\clients.xml 21600000000000 1000000000 CENTRALIZED ARANTES SYMMETRICAL 0.10 FIRST_FIT 3 13 RANDOM
done


# test for the first_fit asymmetrical
for nb_clients in 1000 2000 4000 6000 8000 10000	
do
	./my_boinc ../database_file/boinc_file $nb_workers $nb_clients 13 ../platforms/deployement_centralized_$nb_workers\workers_$nb_clients\clients.xml ../platforms/platform_centralized_$nb_workers\workers_$nb_clients\clients.xml 21600000000000 1000000000 CENTRALIZED ARANTES ASYMMETRICAL 0.10 0.20 FIRST_FIT 3 13 RANDOM
done



# test for the first_fit boinc
for nb_clients in 1000 2000 4000 6000 8000 10000
do
	./my_boinc ../database_file/boinc_file $nb_workers $nb_clients 13 ../platforms/deployement_centralized_$nb_workers\workers_$nb_clients\clients.xml ../platforms/platform_centralized_$nb_workers\workers_$nb_clients\clients.xml 21600000000000 1000000000 CENTRALIZED ARANTES BOINC FIRST_FIT 3 13 RANDOM
done



# test for the first_fit sonnek_reputation

for nb_clients in 1000 2000 4000 6000 8000 10000
do
	./my_boinc ../database_file/boinc_file $nb_workers $nb_clients 13 ../platforms/deployement_centralized_$nb_workers\workers_$nb_clients\clients.xml ../platforms/platform_centralized_$nb_workers\workers_$nb_clients\clients.xml 21600000000000 1000000000 CENTRALIZED ARANTES SONNEK_REPUTATION FIRST_FIT 3 13 RANDOM version_$i
		i=`expr $i + 1`
	done
done
