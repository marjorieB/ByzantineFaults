# test for the tight_fit asymmetrical
for nb_clients in 1000 2000 4000 6000 8000 10000
do
	./my_boinc ../database_file/boinc_file $nb_workers $nb_clients 13 ../platforms/deployement_centralized_$nb_workers\workers_$nb_clients\clients.xml ../platforms/platform_centralized_$nb_workers\workers_$nb_clients\clients.xml 50000000 1000000 CENTRALIZED ARANTES ASYMMETRICAL 0.10 0.20 TIGHT_FIT 3 13 RANDOM
done



# test for the tight_fit boinc

for nb_clients in 1000 2000 4000 6000 8000 10000
do
	./my_boinc ../database_file/boinc_file $nb_workers $nb_clients 13 ../platforms/deployement_centralized_$nb_workers\workers_$nb_clients\clients.xml ../platforms/platform_centralized_$nb_workers\workers_$nb_clients\clients.xml 50000000 1000000 CENTRALIZED ARANTES BOINC TIGHT_FIT 3 13 RANDOM
done



# test for the tight_fit sonnek_reputation
for nb_clients in 51000 2000 4000 6000 8000 10000
do
	./my_boinc ../database_file/boinc_file $nb_workers $nb_clients 13 ../platforms/deployement_centralized_$nb_workers\workers_$nb_clients\clients.xml ../platforms/platform_centralized_$nb_workers\workers_$nb_clients\clients.xml 50000000 1000000 CENTRALIZED ARANTES SONNEK_REPUTATION TIGHT_FIT 3 13 RANDOM
