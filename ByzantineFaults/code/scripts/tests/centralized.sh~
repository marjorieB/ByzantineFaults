#!/bin/bash

cd ../bin


#TEST FOR SONNEK NOT_RANDOM_TARGET_LOC

i=1

# test for the first_fit symmetrical
for nb_workers in 250 500 1000 1500 2000
do
	for nb_clients in 500 1000 1500 2000 5000	
	do
		./my_boinc ../database_file/boinc_file $nb_workers 13 ../platforms/deployement_centralized_$nb_workers\workers_$nb_clients\clients.xml 50000000 1000000 CENTRALIZED SONNEK SYMMETRICAL 0.10 FIRST_FIT 3 13 NOT_RANDOM 0.95 version_$i
		i=`expr $i + 1` 
	done
done


# test for the first_fit asymmetrical
for nb_workers in 250 500 1000 1500 2000
do
	for nb_clients in 500 1000 1500 2000 5000	
	do
		./my_boinc ../database_file/boinc_file $nb_workers 13 ../platforms/deployement_centralized_$nb_workers\workers_$nb_clients\clients.xml 50000000 1000000 CENTRALIZED SONNEK ASYMMETRICAL 0.10 0.20 FIRST_FIT 3 13 NOT_RANDOM 0.95 version_$i
	done
done


# test for the first_fit boinc
for nb_workers in 250 500 1000 1500 2000
do
	for nb_clients in 500 1000 1500 2000 5000	
	do
		./my_boinc ../database_file/boinc_file $nb_workers 13 ../platforms/deployement_centralized_$nb_workers\workers_$nb_clients\clients.xml 50000000 1000000 CENTRALIZED SONNEK BOINC FIRST_FIT 3 13 NOT_RANDOM 0.95 version_$i
	done
done


# test for the first_fit sonnek_reputation
for nb_workers in 250 500 1000 1500 2000
do
	for nb_clients in 500 1000 1500 2000 5000	
	do
		./my_boinc ../database_file/boinc_file $nb_workers 13 ../platforms/deployement_centralized_$nb_workers\workers_$nb_clients\clients.xml 50000000 1000000 CENTRALIZED SONNEK SONNEK_REPUTATION FIRST_FIT 3 13 NOT_RANDOM 0.95 version_$i
	done
done
